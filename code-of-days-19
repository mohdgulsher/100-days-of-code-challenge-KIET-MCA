#include<stdio.h>
void main()
{
int a[3][3],i,j ;
clrscr();
printf("\n enter the value of first matrix::\n");
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
printf("matrix [%d][%d]:",i,j);
scanf("%d",&a[i][j]);
}
}
printf("\n matrix is \n ");
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
printf("\t%d",a[i][j]);
}
printf("\n");
}
getch();
}
